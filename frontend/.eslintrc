{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["plugin:react/recommended"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["import"],
    "rules": {
      "semi": ["error", "never"],
      // "quotes": ["error", "single"],
      "react/react-in-jsx-scope": "off",
      "react/display-name": "off",
      "import/no-webpack-loader-syntax": "error",
      "react/jsx-key": "off",
      "react/prop-types": "off",
      "jsx-a11y/no-static-element-interactions": "off"
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    }
  }
  